mosquito_algorithm <- function(weatherData, rowNumber) {
  data <- weatherData
  likelihoodOfMosquitoes <- as.integer()
  temperature <- as.integer(weatherData[rowNumber,7])
  dewpoint <- as.integer(weatherData[rowNumber,8])
  todayPrecip <- weatherData[rowNumber,9] > 19


  if (temp < 81 & temp > 65) {
    likelihoodOfMosquitoes += 2
  } else if (temp < 95 | temp > 55) {
    likelihoodOfMosquitoes +=1
  }

  if (dewpoint > 50) {
    likelihoodOfMosquitoes +=1
  }

  if (yesterdayPrecip) {
    likelihoodOfMosquitoes += 1
  }

  if (todayPrecip) {
    if (likelihoodOfMosquitoes > 2) {
      likelihoodOfMosquitoes <- 1
    } else {
      likelihoodOfMosquitoes <- 0
    }
  }

  if (likelihoodOfMosquitoes == 3) {
    return "Mosquitoes: Extreme"
  } else if (likelihoodOfMosquitoes == 2) {
    return "Mosquitoes: High"
  } else if(likelihoodOfMosquitoes == 1) {
    return "Mosquitoes: Moderate"
  } else {
    return "Mosquitoes: Low-Negligible"
  }
}
