mosquito_algorithm <- function(weatherData, rowNumber) {
  data <- weatherData
  likelihoodOfMosquitoes <- as.integer()
  temperature <- as.integer(weatherData[rowNumber,3])
  dewpoint <- as.integer("weatherData[rowNumber,4]")
  yesterdayPrecip <- weatherData[rowNumber,5] > 1
  todayPrecip <- weatherData[rowNumber,6] > 19


  if (temp < 81 & temp > 65) {
    likelihoodOfMosquitoes += 2
  } else if (temp < 95 | temp > 55) {
    likelihoodOfMosquitoes +=1
  }

  if (dewpoint > 50) {
    likelihoodOfMosquitoes +=1
  }

  if (yesterdayPrecip) {
    likelihoodOfMosquitoes += 1
  }

  if (todayPrecip) {
    if (likelihoodOfMosquitoes > 3) {
      likelihoodOfMosquitoes <- 1
    } else {
      likelihoodOfMosquitoes <- 0
    }
  }

  return likelihoodOfMosquitoes
}
